    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Inputs
Key_left = keyboard_check_direct (ord("A"));
Key_up = keyboard_check_direct (ord("W"));
Key_right = keyboard_check_direct (ord("D"));
Key_down = keyboard_check_direct (ord("S"));
Key_fire = keyboard_check_direct (vk_control);
Key_chargeBegin = keyboard_check_pressed (vk_control);
Key_chargeEnd = keyboard_check_released (vk_control);
Key_wep1 = keyboard_check_released (ord("1"));
Key_wep2 = keyboard_check_released(ord("2"));
Key_wep3 = keyboard_check_released (ord("3"));

//Movement
if (hp &gt; 0) 
{


//weapon swap

if (Key_wep1 &amp;&amp; weapon != 0) weapon = 0;
if (Key_wep2 &amp;&amp; weapon != 1) weapon = 1;
if (Key_wep3 &amp;&amp; weapon != 2) weapon = 2;

// weapons
switch (weapon)
{
    case 0:
        if (Key_chargeBegin)
        {
            audio_play_sound(snd_machinegun,3,false);
            if (powerup_bullet ==1)
            {
                bullet1 = instance_create (x-15,y,obj_bullet);
                bullet2 = instance_create (x,y-16,obj_bullet);
                bullet3 = instance_create(x+15,y,obj_bullet);
                bullet1.direction = image_angle+2.5;
                bullet2.direction = image_angle;
                bullet3.direction = image_angle-2.5;
            }
            else
            {
                bullet1 = instance_create (x-15,y,obj_bullet);
                bullet2 = instance_create (x+15,y,obj_bullet);  
                bullet1.direction = image_angle;
                bullet2.direction = image_angle;
            }
            alarm[0] = 4;
        
        }
        if (Key_fire &amp;&amp; alarm[0] == -1)
        {
            alarm[0] = 4;
        }
        if (Key_chargeEnd)
        {
            alarm[0] = -1;
        }
        break;
    case 1:
        if (Key_fire &amp;&amp; alarm[3] == -1)
        {
            alarm[3] = 12;
        }      
        if (Key_chargeBegin)
        {
            instance_create (x-1,y-16,obj_charging);
            alarm[3] = 12;
        }  
        if (Key_chargeEnd)
        {
            if (alarm[1] == -1)
            {

            if (powerup_bullet ==0)
            {
                bullet4 = instance_create (x,y-16,obj_laser);
                bullet4.image_xscale = 3;
                bullet4.image_yscale = 3;
            }
            else
            {
            charge = (2 * charge);
                bullet4 = instance_create (x,y-48,obj_laser);
                bullet4.image_xscale = 8;
                bullet4.image_yscale = 8;
            }
            bullet4.direction = image_angle;
            alarm[1] = 30;
            }            
            charge = 0;
        }
        break;
    case 2:
        if (Key_chargeEnd &amp;&amp; alarm[2] == -1)
        {
            alarm[2] = 300;
            missile1 = instance_create(x,y,obj_missile)
            missile1.direction = image_angle;
            instance_create(x,y,obj_missile_reload);
        }
        break;
}

//apparence
if (hp &lt;= 6) image_index = 4;
if (hp &lt;= 12 &amp;&amp; hp &gt; 6) image_index = 3;
if (hp &lt;= 18 &amp;&amp; hp &gt; 12) image_index = 2;
if (hp &lt;= 24 &amp;&amp; hp &gt; 18) image_index = 1;
if (hp &gt; 24) image_index = 0;

//powerup shield
if (powerup_shield == 1)
{
    if (shield_up == 0)
    {
        instance_create(x,y,obj_shield);
        shield_up =1;
        if (alarm[6] == -1)
        {
            alarm[6] = 600;
        }
    }
}

//powerup bullets
    if (alarm[5] == -1)
    {
        alarm[5] = 600;
    }
if (Key_left &amp;&amp; hsp &gt; -3) hsp += -0.3;
if (Key_up &amp;&amp; vsp &gt; -3) vsp += -0.3;
if (Key_right &amp;&amp; hsp &lt; 3) hsp += 0.3;
if (Key_down &amp;&amp; vsp &lt; 3) vsp += 0.3;


if (((Key_left &amp;&amp; Key_right) or (!Key_left &amp;&amp; !Key_right)) &amp;&amp; (hsp != 0)) hsp += -1*sign(hsp)*fric;
if (((Key_up &amp;&amp; Key_down) or (!Key_up &amp;&amp; !Key_down)) &amp;&amp; (vsp != 0)) vsp += -1*sign(vsp)*fric;


//stuck in room

if (place_meeting (x+hsp,y,obj_inv_wall))
{
    while (!place_meeting (x+(2*sign(hsp)),y,obj_inv_wall)) x += sign(hsp);
    hsp = 0;
}
if (place_meeting (x,y+vsp,obj_inv_wall))
{
    while (!place_meeting (x,y+(2*sign (vsp)),obj_inv_wall)) y += sign(vsp);
    vsp = 0;
}

y += vsp;
x += hsp;
}
else  // hp &lt;=0
{
    if (anim_death ==0)
    {
        depth = -1000;
        image_index = 0;
        image_speed = 0.1;
        image_angle = 90;
        audio_play_sound(snd_explosion,1,false);
        sprite_index = spr_explosion;
        alarm [4] = 140;
        anim_death = 1;
    }
}
</string>
          </argument>
        </arguments>
      </action>
